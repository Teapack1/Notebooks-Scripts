\n                # Insert a placeholder if two consecutive messages are from the same speaker\n                placeholder_content = {\"role\": \"assistant\" if last_speaker == \"user\" else \"user\", \"content\": \"[no message]\", \"timestamp\": row[\"Date and Time\"], \"thread\": row[\"Conversation\"]}\n                conversation.append(placeholder_content)\n            \n            message = {\n                \"role\": current_speaker,\n                \"content\": row[\"Content\"],\n                \"timestamp\": row[\"Date and Time\"],\n                \"thread\": row[\"Conversation\"],\n            }\n            conversation.append(message)\n            last_speaker = current_speaker  # Update last speaker\n\n        formatted_data.append({\"conversation_id\": name, \"messages\": conversation})\n\n    return formatted_data\n\n\n# Example usage:\nfile_path = \"/kaggle/input/facebook-raw/all_messages_dataset.csv\"  # Update with the path to your CSV file\ndf_formatted = pd.DataFrame(load_and_preprocess_data(file_path))\n\n# Print the first conversation to see how messages are structured\n#print(df_formatted.iloc[0]['messages'])\n\n# Optional: Save processed data\n# pd.DataFrame(processed_data).to_csv(\"formatted_facebook_messages.csv\", index=False)\n\n\n\nfrom datasets import load_dataset, Dataset, DatasetDict\n\ndataset = Dataset.from_pandas(df_formatted)\n\n\ntokenizer = AutoTokenizer.from_pretrained(\n    \"microsoft/phi-2\",\n    use_fast=True,  # Use Rust-based tokenizer, if availiable\n    trust_remote_code=True,\n    # add_prefix_space=True\n)\n\nsystem_prompt = \"\"\n\n\nimport re\nimport random\nfrom multiprocessing import cpu_count\nfrom datasets import DatasetDict\n\n\ndef apply_chat_template(example, tokenizer, system_prompt=\"\"):\n    messages = example[\"messages\"]\n\n    # Ensure all messages are strings and handle None\n    for message in messages:\n        if message[\"content\"] is None:\n            message[\"content\"] = \"\"  # Conv